<?xml version="1.0"?>
<launch>

    <group ns="ally1">
        <!-- Load this robot's param file with physical props -->
        <!-- This is where you bind a certain robot to be in either the ally1 or ally2 position -->
        <!-- <rosparam command="load" file="$(find playground)/param/$(optenv ROBOT nugget).yaml" /> -->

        <!-- Load this position (ally1/ally2) param file -->
        <rosparam command="load" file="$(find soccersim_mcthuggets)/param/ally1.yaml" />

        <!-- Start the robot up -->
        <include file="$(find soccersim_mcthuggets)/launch/robot.launch">
            <arg name="me" value="ally1" />
            <arg name="ally" value="ally2" />
        </include>

    </group>
</launch>
